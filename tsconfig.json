{
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    // "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
      "node_modules",
      "client/dist",
      "client/**/*.html.ts"
  ]
}
